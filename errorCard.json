{
    "type": "AdaptiveCard",
    "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
    "version": "1.4",
    "body": [
        {
            "type": "Container",
            "items": [
                {
                    "type": "ColumnSet",
                    "columns": [
                        {
                            "type": "Column",
                            "width": "50px",
                            "minHeight": "50px",
                            "verticalContentAlignment": "Center",
                            "items": [
                                {
                                    "type": "Image",
                                    "url": "data:image/png;base64,",
                                    "spacing": "Padding"
                                }
                            ]
                        },
                        {
                            "type": "Column",
                            "width": "stretch",
                            "items": [
                                {
                                    "type": "RichTextBlock",
                                    "inlines": [
                                        {
                                            "type": "TextRun",
                                            "text": "Integration [Employees CRS->DMS]",
                                            "size": "Large",
                                            "weight": "Bolder"
                                        },
                                        {
                                            "type": "TextRun",
                                            "text": " \n"
                                        },
                                        {
                                            "type": "TextRun",
                                            "text": "Failed to complete"
                                        }
                                    ]
                                }
                            ]
                        }
                    ],
                    "style": "emphasis",
                    "bleed": true
                }
            ],
            "style": "default"
        },
        {
            "type": "Container",
            "separator": true,
            "items": [
                {
                    "type": "RichTextBlock",
                    "inlines": [
                        {
                            "type": "TextRun",
                            "text": "Oh it;s bad!!!!!\nno recovery rom this one buddy :( \n- Item 1\r- Item 2\r- Item 3"
                        }
                    ]
                }
            ]
        }
    ],
    "msteams": {
        "width": "Full"
    }
}