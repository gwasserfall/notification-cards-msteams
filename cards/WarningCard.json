{
  "type": "AdaptiveCard",
  "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
  "version": "1.4",
  "body": [
    {
      "type": "Container",
      "items": [
        {
          "type": "ColumnSet",
          "columns": [
            {
              "type": "Column",
              "width": "50px",
              "minHeight": "50px",
              "verticalContentAlignment": "Center",
              "items": [
                {
                  "type": "Image",
                  "url": "data:image/jpeg;base64,b''",
                  "spacing": "Padding"
                }
              ]
            },
            {
              "type": "Column",
              "width": "stretch",
              "items": [
                {
                  "type": "RichTextBlock",
                  "inlines": [
                    {
                      "type": "TextRun",
                      "text": "@{triggerBody()?['title']}",
                      "size": "Large",
                      "weight": "Bolder"
                    },
                    {
                      "type": "TextRun",
                      "text": " \n"
                    },
                    {
                      "type": "TextRun",
                      "text": "@{triggerBody()?['summary']}"
                    }
                  ]
                }
              ]
            }
          ],
          "style": "emphasis",
          "bleed": true
        }
      ],
      "style": "default"
    },
    {
      "type": "Container",
      "separator": true,
      "style": "emphasis",
      "bleed": true,
      "items": [
        {
          "type": "RichTextBlock",
          "inlines": [
            {
              "type": "TextRun",
              "text": "@{triggerBody()?['content']}"
            }
          ]
        }
      ]
    }
  ],
  "msteams": {
    "width": "Full"
  }
}